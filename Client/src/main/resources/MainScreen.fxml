<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.PlayController">
   <children>
      <Label fx:id="usernameLabel" layoutX="14.0" layoutY="14.0" prefHeight="18.0" prefWidth="236.0" text="Hi, " />
      <Label fx:id="paperStatusLabel" layoutX="263.0" layoutY="53.0" prefHeight="18.0" prefWidth="278.0" text="Grade registered!" />
      <ListView fx:id="papersListView" layoutX="45.0" layoutY="155.0" prefHeight="200.0" prefWidth="200.0" />
      <Label fx:id="papersLabel" layoutX="117.0" layoutY="95.0" text="Papers" />
      <Label fx:id="yourGradeLabel" layoutX="405.0" layoutY="147.0" text="Your grade:" />
      <Button fx:id="logOutButton" layoutX="534.0" layoutY="11.0" mnemonicParsing="false" onAction="#logOutHandler" text="Log out" />
      <TextField layoutX="361.0" layoutY="188.0" />
      <Button fx:id="insertGradeButton" layoutX="410.0" layoutY="243.0" mnemonicParsing="false" onAction="#gradeAdded" text="Button" />
   </children>
</AnchorPane>
